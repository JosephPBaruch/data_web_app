{
  "name": "weatherdata",
  "version": "1.0.0",
  "description": "A web app displaying weather data",
  "main": "index.js",
  "scripts": {
    "start-frontend": "nodemon app.js",
    "build_docker_image": "docker build . -t joseph.baruch/image_with_npm_2",
    "docker_nodemon_image": "docker run -it -p 3001:3000 -v $PWD:/usr/src/app joseph.baruch/image_with_npm_2",
    "build_run": "npm run build_docker_image; npm run docker_nodemon_image"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/JosephPBaruch/diseaseData.git"
  },
  "author": "Joseph Baruch",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/JosephPBaruch/diseaseData/issues"
  },
  "homepage": "https://github.com/JosephPBaruch/diseaseData#readme",
  "dependencies": {
    "docker-compose": "^0.24.2",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "nodemon": "^3.0.1"
  }
}
